save_FID_sampling

    _FID_sampling.Sf_category              FID_sampling                                    
    _FID_sampling.ID                       1     
    _FID_sampling.Software_ID		   2
    _FID_sampling.Software_name		   'schedule generator'                          

    _FID_sampling.Experiment_name                          na
    _FID_sampling.NMR_spec_expt_ID                         1          
    _FID_sampling.Layout_dimension_count                   3                                               
    _FID_sampling.Layout_aux_dimension_count               1                                                            
    _FID_sampling.Layout_hypercomplex_component_type       full                                            
    _FID_sampling.Layout_transient_type                    uniform                                         
    _FID_sampling.Expt_quadrature_class                    na                                              
    _FID_sampling.Expt_redfield_trick                      no                                             


    loop_            # Keep schedule base parameters and values in one loop.
        _FID_sampling_base.Spectral_dim       
        _FID_sampling_base.Parameter
        _FID_sampling_base.Parameter_value
        _FID_sampling_base.Param_tag

         2         time_series_type    linear          '_Schedule.Time_index_dim2'
         2         time_multiplier     0.000125         na   
         2         time_offset         0                na 
         2         time_units          sec              na 
         2         phase_series_type   lookup          '_Schedule.Phase_index_dim2'
         2         phase_multiplier    0                na 
         2         phase_offset        0                na 
         2         phase_units         degree           na 
         2         details             ?                na 

         3         time_series_type    linear          '_Schedule.Time_index_dim3'
         3         time_multiplier     0.000250         na     
         3         time_offset         0                na 
         3         time_units          sec              na 
         3         phase_series_type   value           '_Schedule.Phase_value_dim3'
         3         phase_multiplier    0                na 
         3         phase_offset        0                na 
         3         phase_units         degree           na 
         3         details             na               na
 
         AuxA      series_type         lookup          '_Schedule.AuxA_index'
         AuxA      name                tau              na
         AuxA      units               sec              na

    stop_

   
    loop_       # This is the table that contains the NUS schedule                                                                                             
        _Schedule.FID_ID                                                                                                     
        _Schedule.Proc_FID_ID                                                                              
        _Schedule.Time_index_dim2     
        _Schedule.Phase_index_dim2                                                                          
        _Schedule.AuxA_index          
        _Schedule.Time_index_dim3
        _Schedule.Phase_value_dim3
        _Schedule.Transient_count
        _Schedule.FID_weight
        _Schedule.Gap_schedule_dim1                       

        1 1  0  0  0   0   0  16  1.0  gapScheduleA                                                                             
        2 2  1  1  1   1  90  16  1.0  gapScheduleB                                                          
        3 3  2  0  2   2   0  16  1.0  gapScheduleA
        4 4  3  1  3   3  90  16  1.0  gapScheduleB
                                               
    stop_                                                                                               

    loop_             # The following loops provide information for tags that have
                      # the type value set to 'lookup'.

        _Phase_value_dim2.List

          ip  ap

    stop_

    loop_                     # Possible uses: relaxation study
        _AuxA.List
       
       0.0  0.1  0.2  0.4  1.0  2.0 

    stop_

    loop_                          # used for NUS-like 1D burst sampling
        _Gap_schedule.Name
        _Gap_schedule.Ordinal
        _Gap_schedule.Time_point_start
        _Gap_schedule.Time_point_end
     
          gapScheduleA    1   255     300
          gapScheduleA    2   511     580
          gapScheduleA    3  1000    1060
          gapScheduleA    4  1200    1250
          gapScheduleA    5  1520    1590

          gapScheduleB    1   300     360
          gapScheduleB    2   512     590
          gapScheduleB    3  1020    1110
          gapScheduleB    4  1220    1355
          gapScheduleB    5  1500    1575

    stop_

save_

