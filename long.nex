         1    1     obs_freq_at 0ppm                 499.90
         1    1     ppm_at_center                    4.55
         1    1     zero_order_phase_corr            -50.0           
         1    1     first_order_phase_corr           0.0             
         1    1     nucleus                          1H
         1    1     label                            HN
         1    1     details                          na

         2    1     size                             na 
         2    1     spectral_width_Hz                na
         2    1     obs_freq_MHz_at_center           na
         2    1     obs_freq_at 0ppm                 na
         2    1     ppm_at_center                    na
         2    1     zero_order_phase_corr            0.0           
         2    1     first_order_phase_corr           0.0             
         2    1     nucleus                          15N
         2    1     label                            N
         2    1     details                          na

         3    1     size                             na 
         3    1     spectral_width_Hz                na
         3    1     obs_freq_MHz_at_center           na
         3    1     obs_freq_at 0ppm                 na
         3    1     ppm_at_center                    na
         3    1     zero_order_phase_corr            90.0          
         3    1     first_order_phase_corr           -180.0             
         3    1     nucleus                          13C
